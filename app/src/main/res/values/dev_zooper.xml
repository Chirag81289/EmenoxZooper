<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- This info is used in the available skins list in Zooper -->
    <string name="zooper_pack_name">@string/app_name</string>
    <string name="zooper_pack_desc">Beautiful Zooper widgets for your home screen, powered by Emenox.</string>
    <string name="zooper_pack_author">MD Danish Ansari</string>

    <!-- If set to 0, the user will not be allowed to save this template in Zooper -->
    <integer name="zooper_pack_allowsave">1</integer>

    <!-- This is the icon that will be shown in the plugin list in Zooper -->
    <drawable name="zooper_pack_icon">@mipmap/ic_launcher</drawable>

    <!-- Setting to true will display your Zooper Widget templates within Polar -->
    <bool name="enable_zooper_page">true</bool>

    <!-- Optional names of each widget in /assets/templates -->
    <string-array name="zooper_widget_names">
